
class Employee:
    pass
Emp_1 = Employee()
Emp_1.name = input("Enter the name of the employee: ")
Emp_1.salary = 25000  
print('Emp_1.name:', Emp_1.name)
print('Emp_1.salary:', Emp_1.salary)

